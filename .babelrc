{
    "presets": [
      ["env", { "modules": false }],
      "stage-0"
    ],
    "plugins": [
        "transform-runtime",
        [
            "component",
            {
                "libraryName": "element-ui",
                "styleLibraryName": "theme-chalk"
            }
        ]
    ],
    "comments": false,
    "env": {
      "test": {
        "presets": ["env", "stage-0"],
        "plugins": [ "istanbul" ]
      }
    }
  }
